				let code = codes[i];
				if (code.parentNode.tagName !== 'PRE' && code.childElementCount === 0 && /^\$+[^$]*\$+$/.test(code.textContent)) {
					code.outerHTML = code.innerHTML;
				}
			}

			var script = document.createElement("script");
			script.type = "text/javascript";
